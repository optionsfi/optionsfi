{
    "name": "@optionsfi/oracle",
    "version": "0.1.0",
    "description": "Hybrid pricing oracle for OptionsFi with on-chain and TradFi data integration",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "test": "vitest",
        "lint": "tsc --noEmit",
        "setup": "tsx scripts/setup.ts",
        "sample": "tsx scripts/sample-prices.ts",
        "test:vol": "tsx scripts/test-volatility.ts"
    },
    "keywords": [
        "oracle",
        "pricing",
        "volatility",
        "options",
        "solana",
        "defi"
    ],
    "author": "OptionsFi Team <team@optionsfi.xyz>",
    "license": "MIT",
    "dependencies": {
        "@pythnetwork/client": "^2.21.0",
        "@pythnetwork/hermes-client": "^1.0.0",
        "@solana/web3.js": "^1.95.8",
        "@supabase/supabase-js": "^2.39.0",
        "axios": "^1.6.2"
    },
    "devDependencies": {
        "@types/node": "^20.10.5",
        "dotenv": "^17.2.3",
        "tsup": "^8.0.1",
        "tsx": "^4.21.0",
        "typescript": "^5.3.3",
        "vitest": "^1.0.4"
    }
}
