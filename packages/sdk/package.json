{
    "name": "@optionsfi/sdk",
    "version": "0.1.0",
    "description": "OptionsFi SDK - On-chain options settlement infrastructure for Solana",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "test": "vitest run",
        "test:watch": "vitest",
        "lint": "eslint src --ext .ts",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "solana",
        "options",
        "defi",
        "rfq",
        "derivatives",
        "trading",
        "sdk"
    ],
    "author": "OptionsFi Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/feeniks01/optionsfi"
    },
    "homepage": "https://optionsfi.xyz",
    "bugs": {
        "url": "https://github.com/feeniks01/optionsfi/issues"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "peerDependencies": {
        "@coral-xyz/anchor": "^0.29.0",
        "@solana/web3.js": "^1.87.0"
    },
    "dependencies": {
        "ws": "^8.14.0"
    },
    "devDependencies": {
        "@coral-xyz/anchor": "^0.29.0",
        "@solana/web3.js": "^1.87.0",
        "@types/node": "^20.0.0",
        "@types/ws": "^8.5.0",
        "tsup": "^8.0.0",
        "typescript": "^5.3.0",
        "vitest": "^1.0.0"
    }
}